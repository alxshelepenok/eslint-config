{
  "name": "@alxshelepenok/eslint-config",
  "description": "My shareable eslint config",
  "version": "0.4.8",
  "main": "index.js",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --ext .js src",
    "format": "eslint --cache --ext .js src --fix",
    "semantic-release": "semantic-release",
    "test": "tape ./src/*.test.js"
  },
  "engines": {
    "node": ">= 12 || ^10.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alxshelepenok/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.1",
    "eslint": "8.2.0",
    "tape": "5.3.1",
    "lint-staged": "11.1.4",
    "prettier": "2.4.1",
    "prettier-plugin-packagejson": "2.2.12",
    "semantic-release": "18.0.0",
    "husky": "7.0.4"
  },
  "peerDependencies": {
    "eslint": "^7.7.0 || ^8.2.0"
  }
}
